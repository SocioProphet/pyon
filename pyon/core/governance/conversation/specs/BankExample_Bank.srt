protocol BuyBond(role client, role bank, role resource_registry) {
	buy_bonds from Client;
	read to resource_registry;
	(Account) from resource_registry;
	choice {
		(Exception) to resource_registry;}
	or  {
	    find_subjects to resource_registry;
	    (CustomerId) from resource_registry;
	    exercise(Order) to trade;
	    (Confirmation) from trade;
	    choice {
	    	update to resource_registry;
	    	(status:string) to client;}
	    or {(status:string) to client;}}
}